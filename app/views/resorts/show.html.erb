<h1>Resort Information</h1>

<div class="container">
  <div class="info_container">
    <div class="info">ID: <%= @resort.id %></div>
    <div class="info">Name: <%= @resort.name %></div>
    <div class="info">Latitude: <%= @resort.latitude %></div>
    <div class="info">Longitude: <%= @resort.longitude %></div>
    <div class="info">State: <%= @resort.state %></div>
  </div>
  <%= link_to "Index", resorts_path %>
</div>

