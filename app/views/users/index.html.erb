<h1>Users Index</h1>

<div class="container">
<% @users.each do |u| %>
  <div class="id"><%= u.id %>:</div>
  <div class="username"><%= u.username %></div>
  <%= link_to "Info", user_path(u) %>
  <%= link_to "Edit", edit_user_path(u) %>
  <%= link_to "Delete", user_path(u), method: :delete, data: {confirm: "Are you sure?"} %>
<% end %>
</div>